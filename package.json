{
   "name": "messman",
   "version": "0.1.0",
   "private": true,
   "scripts": {
      "dev": "next dev",
      "build": "next build",
      "start": "next start",
      "lint": "next lint",
      "test": "echo null",
      "prepare": "npx is-ci || husky install",
      "preci": "rm -rf node_modules",
      "ci": "yarn install --frozen-lockfile",
      "format": "prettier --write ./src",
      "clean": "rm -rfv .next/"
   },
   "lint-staged": {
      "*.{ts,tsx}": "yarn test --watchAll=false --findRelatedTests",
      "*.{js,jsx,ts,tsx,json,md,html,yaml}": "yarn prettier --write"
   },
   "dependencies": {
      "@emotion/cache": "^11.10.5",
      "@emotion/react": "^11.10.5",
      "@emotion/styled": "^11.10.5",
      "@mui/icons-material": "^5.11.0",
      "@mui/material": "^5.11.3",
      "@mui/x-date-pickers": "^5.0.17",
      "@next/font": "13.1.1",
      "date-fns": "^2.29.3",
      "next": "13.1.1",
      "react": "18.2.0",
      "react-day-picker": "^8.4.1",
      "react-dom": "18.2.0",
      "react-router-dom": "^6.6.1",
      "typescript": "4.9.4"
   },
   "devDependencies": {
      "@types/node": "18.11.18",
      "@types/react": "18.0.26",
      "@types/react-dom": "18.0.10",
      "eslint": "8.31.0",
      "eslint-config-next": "13.1.1",
      "husky": "^8.0.3",
      "lint-staged": "^13.1.0",
      "prettier": "^2.8.2"
   },
   "prettier": {
      "tabWidth": 3,
      "singleQuote": true,
      "jsxSingleQuote": true,
      "arrowParens": "avoid",
      "endOfLine": "lf"
   }
}
