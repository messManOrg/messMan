{
   "name": "messman",
   "version": "0.1.0",
   "private": true,
   "scripts": {
      "dev": "next dev --port 3001",
      "build": "next build",
      "start": "next start",
      "lint": "next lint",
      "test": "echo null",
      "prepare": "npx is-ci || husky install",
      "preci": "rm -rf node_modules",
      "ci": "yarn install --frozen-lockfile",
      "format": "prettier --write",
      "precommit": "yarn lint --fix && yarn format",
      "clean": "rm -rfv .next/"
   },
   "lint-staged": {
      "*.{ts,tsx}": "yarn test --watchAll=false --findRelatedTests",
      "*.{js,jsx,ts,tsx,json,md,html,yaml}": "yarn precommit"
   },
   "dependencies": {
      "@emotion/cache": "^11.10.5",
      "@emotion/react": "^11.10.5",
      "@emotion/styled": "^11.10.5",
      "@mui/icons-material": "^5.11.0",
      "@mui/material": "^5.11.3",
      "@mui/x-date-pickers": "^5.0.17",
      "@next/font": "13.1.1",
      "axios": "^1.3.4",
      "date-fns": "^2.29.3",
      "next": "13.1.1",
      "react": "18.2.0",
      "react-day-picker": "^8.4.1",
      "react-dom": "18.2.0",
      "react-error-boundary": "^4.0.2",
      "react-router-dom": "^6.6.1",
      "swr": "^2.1.0",
      "typescript": "4.9.4"
   },
   "devDependencies": {
      "@trivago/prettier-plugin-sort-imports": "^4.1.1",
      "@types/node": "18.11.18",
      "@types/react": "18.0.26",
      "@types/react-dom": "18.0.10",
      "@typescript-eslint/eslint-plugin": "^5.56.0",
      "@typescript-eslint/parser": "^5.56.0",
      "eslint": "8.31.0",
      "eslint-config-next": "13.1.1",
      "eslint-plugin-unused-imports": "^2.0.0",
      "husky": "^8.0.3",
      "lint-staged": "^13.1.0",
      "prettier": "^2.8.2"
   },
   "prettier": {
      "tabWidth": 3,
      "singleQuote": true,
      "jsxSingleQuote": true,
      "arrowParens": "avoid",
      "trailingComma": "es5",
      "endOfLine": "lf",
      "importOrder": [
         "^react",
         "^next/(.*)$",
         "^@mui/(.*)$",
         "<THIRD_PARTY_MODULES>",
         "^com/(.*)$",
         "^[../]",
         "^[./]"
      ]
   }
}
